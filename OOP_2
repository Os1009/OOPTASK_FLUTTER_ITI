class Animal {
  void speak() {
    print('Animal sound');
  }
}

class Dog extends Animal {
  @override
  void speak() {
    print('Woof');
  }
}

class Cat extends Animal {
  @override
  void speak() {
    print('Meow');
  }
}

void main() {
  List<Animal> animals = [Dog(), Cat(), Dog(), Cat()];
  for (var animal in animals) {
    animal.speak();
  }
}
